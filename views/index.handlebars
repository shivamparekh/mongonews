<div class="container">

	<table>
		<tbody>

			{{#each articles}}

				<div class=row>

					<div class="col s9">

						<a href="{{this.link}}">{{this.title}}</a>

					</div>

					<div class="col s3">

						<button class="note" id="{{this._id}}">notes</button>

					</div>

				</div>

			{{/each}}

		</tbody>
	</table>
</div>